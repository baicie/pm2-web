{
    "name": "@baicie/pm2-web",
    "version": "0.1.2",
    "description": "pm2 web server",
    "type": "commonjs",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "pnpm -r --filter=./client run build && pnpm -r --filter=./server run build",
        "server:client": "pnpm -r --filter=./client run preview",
        "server:server": "pnpm -r --filter=./server run prod",
        "server": "concurrently \"npm run server:client\" \"npm run server:server\"",
        "lint": "pnpm -r --filter=./client run lint && pnpm -r --filter=./server run lint",
        "format": "pnpm -r --filter=./client run format && pnpm -r --filter=./server run format"
    },
    "author": "baicie",
    "files": [
        "scripts",
        "bin",
        "client/dist",
        "server/dist",
        "client/package.json",
        "server/package.json"
    ],
    "license": "MIT",
    "keywords": [
        "pm2 web"
    ],
    "bin": {
        "pm2-web": "bin/pm2-web"
    },
    "dependencies": {
        "@element-plus/icons-vue": "^2.0.10",
        "@hapi/boom": "^10.0.1",
        "@vue/shared": "^3.2.47",
        "@vueuse/core": "^9.13.0",
        "axios": "^1.3.4",
        "body-parser": "^1.20.2",
        "concurrently": "^7.6.0",
        "cors": "^2.8.5",
        "dayjs": "^1.11.7",
        "element-plus": "^2.2.32",
        "express": "^4.18.2",
        "express-jwt": "^8.4.1",
        "express-validator": "^6.15.0",
        "i18n": "^0.15.1",
        "jsonwebtoken": "^9.0.0",
        "lodash-es": "^4.17.21",
        "nprogress": "^0.2.0",
        "pinia": "^2.0.32",
        "pm2": "^5.2.2",
        "reflect-metadata": "^0.1.13",
        "shell": "^0.5.1",
        "shelljs": "^0.8.5",
        "sql.js": "^1.8.0",
        "ts-node": "^10.9.1",
        "typeorm": "^0.3.12",
        "vue": "^3.2.45",
        "vue-i18n": "^9.2.2",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
